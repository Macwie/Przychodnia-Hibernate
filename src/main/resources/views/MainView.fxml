      <Button fx:id="manageButton" layoutX="82.0" layoutY="386.0" mnemonicParsing="false" onAction="#openManageView" opacity="0.0" prefHeight="150.0" prefWidth="250.0" styleClass="button" text="Zarządzaj Danymi">
